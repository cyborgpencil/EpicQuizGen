<Window x:Class="EpicQuizGen.Views.CompleteQuizView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EpicQuizGen.Views"
        xmlns:vm="clr-namespace:EpicQuizGen.ViewModels"
        mc:Ignorable="d"
        Title="Quiz Results" Height="100" Width="300">
    <StackPanel>
        <TextBlock Text="Your Score"/>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Quiz Name: "/>
            <TextBlock Text="{Binding Path=Quiz.QuizName}" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Quiz Score: "/>
            <TextBlock Text="{Binding Path=Quiz.Grade}" />
        </StackPanel>
    </StackPanel>
</Window>
