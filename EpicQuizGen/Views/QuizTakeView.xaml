<UserControl x:Class="EpicQuizGen.Views.QuizTakeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EpicQuizGen.Views"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d" 
             d:DesignHeight="640" d:DesignWidth="1000" Background="Gray">

    <StackPanel MaxWidth="1000">
        <TextBlock Text="Time Left: 00" Margin="2" HorizontalAlignment="Right" Background="White" FontSize="30" TextAlignment="Center"/>
    </StackPanel>
</UserControl>
